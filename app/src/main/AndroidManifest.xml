<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tuitionfee">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TuitionFee">
        <activity android:name=".LoanItemActivity"></activity>
        <activity android:name=".AddLoanActivity" />
        <activity android:name=".ManageLoanActivity" />
        <activity android:name=".AddSubjectActivity" />
        <activity android:name=".SubjectItemActivity" />
        <activity android:name=".ManageSubjectActivity" />
        <activity android:name=".StudyingItemActivity" />
        <activity android:name=".StudyingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>